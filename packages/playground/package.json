    "build:no": "tsc --noEmit",
    "run": "esrun src/index.ts"
  },
  "devDependencies": {
    "@digitak/esrun": "^3.2.24",
    "@types/node": "^20.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@trevthedev/toolbelt": "workspace:^"
  },
  "engines": {
    "node": ">=18"
  },
