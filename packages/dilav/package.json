{
  "name": "dilav",
  "type": "module",
  "version": "0.0.5",
  "description": "A blazing fast way to transforms `unknown` types, into valid known typescript types.",
  "homepage": "https://github.com/TrevTheDev/monorepo/tree/main/packages/dilav#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/TrevTheDev/monorepo.git",
    "directory": "packages/dilav"
  },
  "bugs": {
    "url": "https://github.com/TrevTheDev/monorepo/issues"
  },
  "keywords": [
    "typescript",
    "schema",
    "validation",
    "type",
    "inference"
  ],
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./types": "./dist/types/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "build": "esbuild ./src/index.ts --bundle --outfile=./dist/index.js --minify --tree-shaking=true",
    "build:types": "tsc -d --declarationDir dist/types --declarationMap --emitDeclarationOnly",
    "buildxx": "tsup ./src/index.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && npm run build",
    "preversion": "npm run lint",
    "publish": "npm version patch && git push --tags origin main",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "TrevTheDev",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "2.8.8",
    "ts-unused-exports": "^9.0.4",
    "tsconfig": "*",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.30.1",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=19.0.0"
  },
  "dependencies": {
    "@trevthedev/toolbelt": "*"
  }
}
